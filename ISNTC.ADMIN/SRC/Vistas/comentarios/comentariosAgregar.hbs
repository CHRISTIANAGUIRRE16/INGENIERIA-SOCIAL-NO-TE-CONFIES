<link rel="stylesheet" href="/css/comentarios/comentariosAgregar.css">
<div id="fond">
    <img src="/imagenes/fondo.jpeg" alt="">
</div>
<div id="contenedor">
    <form action="/comentarios/comentariosAgregar/{{user.idUsuarios}}" method="post">
    <div id="titulo">
        <p>Agregar Comentarios</p>
    </div>
    <div id="AgregarNombre">
        <label>Nombre:</label>
        <input type="text" name="agregarNombre" required>
    </div>
    <div id="AgregarExperiencias">
        <label>Describa su experiencia:</label>
        <input type="text" name="agregarExperiencias" required>
    </div>
    <div id="boton">
        <button type="submit">GUARDAR</button>
    </div>
    </form>
</div>