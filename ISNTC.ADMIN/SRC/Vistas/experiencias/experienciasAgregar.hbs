<link rel="stylesheet" href="/css/experiencias/experienciasAgregar.css">
<div id="fond">
    <img src="/imagenes/fondo.jpeg" alt="">
</div>
<div id="contenedor">
    <form action="/experiencias/experienciasAgregar/{{user.idUsuarios}}" method="post">
    <div id="titulo">
        <p>Agregar experiencias</p>
    </div>
    <div id="AgregarNombre">
        <label>Nombre:</label>
        <input type="text" name="agregarNombre" required>
    </div>
    <div id="AgregarExperiencia">
        <label>Describa su experiencia:</label>
        <input type="text" name="agregarExperiencia" required>
    </div>
    <div id="boton">
        <button type="submit">GUARDAR</button>
    </div>
    </form>
</div>