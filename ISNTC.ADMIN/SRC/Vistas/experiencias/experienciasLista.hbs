<link rel="stylesheet" href="/css/experiencias/experienciasLista.css">

<form action="/experiencias/experienciasLista/{{user.idUsuarios}}" method="get"></form>
<div id="contenedor">
    <div class="row">
        {{#each lista}}
        <div id="tarjeta">
            <div id="tema">
                <p>{{temaExperiencias}}</p>
            </div>
            <div id="fecha">
                <p>{{fechaExperiencias}}</p>
            </div>
            <div id="video">
                <p>{{videoExperiencias}}</p>
            </div>
            <div id="boton">
                <div id="editar">
                    <a href="/experiencias/experienciasEditar/{{idExperiencias}}"><button type="button">Editar</button></a>
                </div>
                <div id="eliminar">
                    <a href="/experiencias/eliminar/{{idExperiencias}}"><button type="button">Eliminar</button></a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>